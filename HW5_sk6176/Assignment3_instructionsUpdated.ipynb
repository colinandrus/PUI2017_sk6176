    "            yerr = np.sqrt(df['Average total all civilian firearms'] * 1.0), fmt = '.')\n",
    "ax.set_xlabel(\"gdp\")\n",
    "ax.set_ylabel(\"Average total all civilian firearms\")\n",
    "ax.set_ylim()\n",
    "pl.show()"
