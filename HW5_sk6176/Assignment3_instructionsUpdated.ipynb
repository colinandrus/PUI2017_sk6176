    "    else:\n",
    "        col.append(\"y\")\n",
    "        \n",
    "fig = pl.figure(figsize=(10,5)) \n",
    "ax = fig.add_subplot(111)\n",
    "\n",
    "scatter = ax.scatter(df['Number of homicides by firearm'], df['Average total all civilian firearms'],color =col)\n",
